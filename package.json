{
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "project/*",
      "setup/*"
    ]
  },
  "dependencies": {
    "eslint": "8.14.0",
    "prettier": "2.6.2",
    "@types/styled-components": "5.1.25",
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "typescript": "4.5.5"
  },
  "scripts": {
    "lint": "yarn workspaces run lint",
    "web-public": "yarn workspace @lacosdeluana/web-public",
    "lint:web-public": "yarn web-public lint",
    "dev:web-public": "yarn web-public dev",
    "build:web-public": "yarn web-public build",
    "start:web-public": "yarn web-public start",
    "design-system": "yarn workspace @lacosdeluana/design-system"
  }
}
